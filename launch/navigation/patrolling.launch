<?xml version="1.0"?>
<launch>

    <arg name="robot_ns" default=""/>

    <rosparam command="load" file="$(find multi_sim)/config/patparams.yaml" />

    <group ns="$(arg robot_ns)">

        <!-- <include file="$(find auto_nav)/launch/explore.launch">
            <arg name="robot_ns" value="$(arg robot_ns)"/>    
        </include> -->

        <node pkg="multi_sim" type="patrolling_legacy.py" name="patroller" output="screen"/> 

        <include file="$(find multi_sim)/launch/plasticpat.launch" >
            <arg name="robot_ns" value="$(arg robot_ns)"/>    
        </include>

    </group>
</launch>